<h1>All Shelters</h1>

<% @shelters.each do |shelter| %>
  <section id="shelter-<%= shelter.id %>">
    <h3><%= link_to shelter.name, "/shelters/#{shelter.id}" %></h3>
    <%= link_to "Edit", "/shelters/#{shelter.id}/edit" %><br/>
  </section>
<% end %>

<%= link_to "New Shelter", "/shelters/new" %>
